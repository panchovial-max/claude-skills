# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_KEY=your-service-role-key

# Netlify
URL=https://your-site.netlify.app

# Google Ads OAuth
GOOGLE_ADS_CLIENT_ID=your-google-ads-client-id
GOOGLE_ADS_CLIENT_SECRET=your-google-ads-client-secret
GOOGLE_ADS_DEVELOPER_TOKEN=your-google-ads-developer-token

# Meta (Facebook + Instagram) OAuth
META_APP_ID=your-meta-app-id
META_APP_SECRET=your-meta-app-secret

# LinkedIn OAuth
LINKEDIN_CLIENT_ID=your-linkedin-client-id
LINKEDIN_CLIENT_SECRET=your-linkedin-client-secret

# TikTok OAuth
TIKTOK_CLIENT_KEY=your-tiktok-client-key
TIKTOK_CLIENT_SECRET=your-tiktok-client-secret

# Notion Internal Integration (for reading client content calendars)
# Create at: https://www.notion.so/my-integrations (Internal Integration, not OAuth)
NOTION_INTERNAL_TOKEN=ntn_your_internal_integration_token

# Google Calendar OAuth (for creating client calendars)
# Configured at: https://console.cloud.google.com/apis/credentials
# Client ID: 1077601603611-9q44lcu2ot1f9td1010m12a0ro4a8h7p.apps.googleusercontent.com
GOOGLE_CALENDAR_CLIENT_ID=1077601603611-9q44lcu2ot1f9td1010m12a0ro4a8h7p.apps.googleusercontent.com
GOOGLE_CALENDAR_CLIENT_SECRET=your-client-secret-from-json-file
GOOGLE_CALENDAR_REDIRECT_URI=${URL}/.netlify/functions/oauth-google-calendar-callback

# Google Calendar for PVB (your authorized account tokens - set after first auth)
GOOGLE_CALENDAR_ACCESS_TOKEN=
GOOGLE_CALENDAR_REFRESH_TOKEN=
